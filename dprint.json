{
  "$schema": "https://dprint.dev/schemas/v0.json",
  "projectType": "openSource",
  "includes": ["**/*.md"],
  "excludes": [
    "node_modules",
    "web/node_modules",
    "web/dist",
    "target"
  ],
  "plugins": [
    "https://plugins.dprint.dev/markdown-0.16.3.wasm"
  ],
  "markdown": {
    "textWrap": "maintain",
    "lineWidth": 100
  }
}
